@model  IEnumerable<Assignment5_B_Diarra.Models.ClassModule>

@{
    ViewBag.Title = "classList";
}

<h2>List of the @Model.Count() classes from the database</h2>
<ol>
    @foreach (var item in Model)
    {
        <li>
            <a href="/Classes/ShowClass/@item.classId"> @item.classCode : @item.className</a>
        </li>
    }
</ol>



